{
  "name": "antlr4-shulk",
  "version": "0.4.0",
  "description": "A library to autocomplete antlr4 grammars",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "retest": "del ./test/tmp/cache.json; npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielBV/antlr4-shulk.git"
  },
  "author": "Daniel Bazaco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielBV/antlr4-shulk/issues"
  },
  "homepage": "https://github.com/DanielBV/antlr4-shulk#readme",
  "dependencies": {
    "antlr4": "^4.11.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!antlr4)"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/core": "^7.19.6",
    "babel-jest": "^29.2.1",
    "jest": "^29.2.1"
  }
}
